{
  "eslint.enable": true,
  "prettier.enable": true,
  "editor.formatOnSave": true,
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "eslint.validate": [
    "vue",
    "yaml",
    "json",
    "jsonc",
    "json5",
    "svelte",
    "markdown",
    "javascript",
    "typescript",
    "javascriptreact",
    "typescriptreact"
  ],
  "cSpell.words": [
    "bilibili",
    "bumpp",
    "changelogithub",
    "noreply",
    "ofetch",
    "pnpm",
    "stefanzweifel",
    "tseslint",
    "vmid"
  ],
  "cSpell.allowCompoundWords": true
}
