{
  "eslint.validate": [
    "javascript",
    "typescript",
    "javascriptreact",
    "typescriptreact",
    "vue",
    "yaml",
    "html",
    "markdown",
    "json",
    "jsonc",
    "json5"
  ],
  "[typescript]": {
    "editor.formatOnSave": false
  },
  "cSpell.words": [
    "bilibili",
    "bumpp",
    "changelogithub",
    "noreply",
    "ohmyfetch",
    "pnpm",
    "stefanzweifel"
  ],
  "cSpell.allowCompoundWords": true
}
